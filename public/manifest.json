{
    "manifest_version": 3,
    "name": "Tab Board",
    "version": "0.1.0",
    "description": "Tab Board",
    "action": {
      "default_popup": "index.html"
    },
    "permissions": [
        "activeTab",
        "tabs",
        "storage",
        "scripting",
        "contextMenus",
        "http://*/*",
        "https://*/*",
        "<all_urls>"
    ],
    "host_permissions": [
        "<all_urls>",
        "http://*/*",
        "https://*/*"
    ],
    "background": {
        "service_worker": "background/background.js"
    }
  }