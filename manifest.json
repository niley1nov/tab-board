{
    "manifest_version": 3,
    "name": "Tab Board",
    "version": "0.1",
    "description": "Tab Board",
    "action": {
      "default_popup": "src/popup.html"
    },
    "permissions": [
      "activeTab",
      "tabs"
    ],
    "background": {
        "service_worker": "src/background.js"
    }
  }